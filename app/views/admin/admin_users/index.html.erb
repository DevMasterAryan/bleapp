 <section class="content-header">
      <h1>
        Admin Users
      </h1>
      <h1>
        <form action="<%= admin_admin_users_path %>"  method="get" >
            <input type="text" name="search" placeholder="Search user" class="form-control" > 
          <button class="btn btn-black">Search</button>
      </form>
        Data Tables
        <small>advanced tables</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Tables</a></li>
        <li class="active">Data tables</li>
      </ol>
    </section>
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">
                <%= form_tag import_admin_admin_users_path, multipart: true do %>
                <%= file_field_tag :file, required: true %>
              </h3>
                <%= submit_tag "Import" %>
                <%end%>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example2" class="table table-bordered table-hover">
                <thead>
                <tr>
                  <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Department</th>
                    <th>Designation</th>
                    <th>Territory</th>
                </tr>
                </thead>
                <tbody>
                <% @admin_users.each do |admin_user|%>
                  <tr>
                    <td><%= admin_user.try(:first_name)%></td>
                    <td><%= admin_user.try(:last_name)%></td>
                    <!-- <td><span class="tag tag-success">Approved</span></td> -->
                    <td><%= admin_user.try(:email)%></td>
                    <td><%= admin_user.try(:department)%></td>
                    <td><%= admin_user.try(:designation)%></td>
                    <td><%= admin_user.try(:territory)%></td>
                  </tr>
                  <%end%>
                </tbody>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
